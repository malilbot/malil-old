{
	"name": "malil",
	"version": "1.0.0",
	"description": "Malil discord bot",
	"main": "dist/index.js",
	"scripts": {
		"lintfix": "eslint . --ext .ts --fix",
		"parsemd": "node dist/Lib/parseMd.js",
		"format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{ts,jsx}\"",
		"start": "tsc && node ./dist/index.js",
		"build": "tsc",
		"enmap": "sh ./resources/fixEnmap.sh",
		"test": "jest",
		"lint": "eslint . --ext .ts",
		"dir": "sh resources/mkdir.sh",
		"dev": "nodemon -e ts --exec \"yarn start\"",
		"deb": "nodemon -e ts --exec \"yarn start\"",
		"vps": "npm run build && npm start",
		"rm": "rm -r dist/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SkyBlockDev/malil-akairo"
	},
	"keywords": [
		"akairo",
		"discordbot",
		"tricked"
	],
	"author": "tricked#3777",
	"license": "UNLICENSED",
	"private": true,
	"bugs": {
		"url": "https://github.com/SkyBlockDev/malil-akairo/issues"
	},
	"_moduleAliases": {
		"@malil/lib": "dist/Lib",
		"@malil/root": "dist"
	},
	"homepage": "https://github.com/SkyBlockDev/malil-akairo#readme",
	"devDependencies": {
		"@types/centra": "^2.2.0",
		"@types/cron": "^1",
		"@types/node": "^15.0.1",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"babel-eslint": "^10.1.0",
		"discord-rpc": "^3.2.0",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.3.1",
		"nodemon": "^2.0.7",
		"prettier": "^2.3.0",
		"prettier-eslint-cli": "^5.0.1",
		"typescript": "^4.2.4"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.3",
		"utf-8-validate": "^5.0.4",
		"zlib-sync": "^0.1.7"
	},
	"dependencies": {
		"centra": "^2.4.2",
		"chalk": "^4.1.0",
		"cheerio": "^1.0.0-rc.9",
		"common-tags": "^1.8.0",
		"cron": "^1.8.2",
		"discord-akairo": "SkyBlockDev/discord-akairo",
		"discord-api-types": "^0.18.0",
		"discord.js": "discordjs/discord.js",
		"enmap": "^5.8.5",
		"fastify": "^3.15.1",
		"fastify-helmet": "^5.3.1",
		"knex": "^0.95.5",
		"moment": "^2.29.1",
		"pg": "^8.6.0",
		"tslib": "^2.2.0"
	},
	"engines": {
		"node": ">=14.15.1"
	},
	"eslintConfig": {
		"env": {
			"es2021": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"plugins": [
			"@typescript-eslint"
		],
		"rules": {
			"require-await": "warn",
			"no-return-await": "warn",
			"no-empty-function": "off",
			"@typescript-eslint/no-empty-function": [
				"warn"
			],
			"no-empty": "warn",
			"@typescript-eslint/no-empty-interface": "warn"
		}
	},
	"prettier": {
		"tabWidth": 4,
		"singleQuote": false,
		"semi": true,
		"bracketSpacing": true,
		"arrowParens": "always",
		"printWidth": 200,
		"useTabs": true,
		"endOfLine": "lf"
	}
}
