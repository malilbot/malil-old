{
	"name": "malil",
	"version": "1.0.0",
	"description": "Malil discord bot",
	"main": "dist/index.js",
	"scripts": {
		"lintfix": "eslint . --ext .ts --fix",
		"format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx}\"",
		"start": "yarn estsc && node ./dist/index.js",
		"devstart": "yarn devestc && node ./dist/index.js",
		"build": "tsc",
		"estsc": "yarn esbuild --outdir=dist --platform=node --minify-whitespace --minify-syntax --target=es2020 --format=cjs src/**/*.ts",
		"devestc": "yarn esbuild --outdir=dist --platform=node --sourcemap=inline --target=es2020 --format=cjs src/**/*.ts",
		"lint": "eslint . --ext .ts",
		"dir": "sh resources/mkdir.sh",
		"dev": "nodemon -e ts --exec \"yarn devstart\"",
		"deb": "nodemon -e ts --exec \"yarn devstart\"",
		"vps": "npm run build && npm start",
		"rm": "rm -r dist/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/malilbot/malil"
	},
	"keywords": [
		"akairo",
		"discordbot",
		"tricked"
	],
	"author": "tricked#3777",
	"license": "ARR",
	"private": true,
	"bugs": {
		"url": "https://github.com/malilbot/malil/issues"
	},
	"_moduleAliases": {
		"@malil/lib": "dist/Lib",
		"@malil/root": "dist"
	},
	"resolutions": {
		"typescript": "4.1.5"
	},
	"homepage": "https://github.com/malilbot/malil#readme",
	"devDependencies": {
		"@types/node": "^15.3.0",
		"@typescript-eslint/eslint-plugin": "^4.24.0",
		"@typescript-eslint/parser": "^4.24.0",
		"discord-api-types": "^0.18.1",
		"esbuild": "^0.12.2",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"nodemon": "^2.0.7",
		"prettier": "^2.3.0",
		"prettier-eslint-cli": "^5.0.1",
		"tslib": "^2.2.0",
		"typescript": "next"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.3",
		"utf-8-validate": "^5.0.5",
		"zlib-sync": "^0.1.7"
	},
	"dependencies": {
		"chalk": "^4.1.1",
		"cheerio": "^1.0.0-rc.9",
		"discord-akairo": "SkyBlockDev/discord-akairo",
		"discord.js": "discordjs/discord.js",
		"enmap": "^5.8.5",
		"knex": "^0.95.6",
		"moment": "^2.29.1",
		"petitio": "^1.3.2",
		"pg": "^8.6.0",
		"tslog": "^3.2.0"
	},
	"engines": {
		"node": ">=14.15.1"
	},
	"eslintConfig": {
		"env": {
			"es2021": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"plugins": [
			"@typescript-eslint"
		],
		"rules": {
			"require-await": "warn",
			"no-return-await": "warn",
			"no-empty-function": "off",
			"@typescript-eslint/no-empty-function": [
				"warn"
			],
			"no-empty": "warn",
			"@typescript-eslint/no-empty-interface": "warn"
		}
	},
	"prettier": {
		"tabWidth": 4,
		"singleQuote": false,
		"semi": true,
		"bracketSpacing": true,
		"arrowParens": "always",
		"printWidth": 200,
		"useTabs": true,
		"endOfLine": "lf"
	}
}
